package myWhile;

public class NoteSlide11 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
        int x=0;
        int y=1;
        int m=0;
        int n=1;
        
        while (y <=10) {
        	x=x+y;
        	y=y+5;
        	System.out.println(x+ " ");
        }
        
        do {
        	m=m+n;
        	n=n+5;
        	System.out.println(m+ " ");
        } while (n<=10);
        	
        }
	}


